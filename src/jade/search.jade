include mixin/source

extends ./index.jade
block content
  #Search
    h1 Search

    +source('Lookup anything')
      input(type='search', placeholder='Search')

    +source('input-search')
      input.input-search(type='text', placeholder='Search')

    +source('Search_button')
      form.form(action='')
        .input-groups
          input(type='text', name='go', placeholder='Search')
          span.btn-append
            button.btn Go

    +source('search input-on-black')
      input.input-on-black(type='search', placeholder='Type to search...')

